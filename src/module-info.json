{
    "name": "tactics.tools",
    "id": "aarontburn.Tactics_Tools",
    "author": "aarontburn",
    "version": "1.0.0",
    "description": "Embed tactics.tools for quick TFT lookups",
    "platforms": [],
    "link": "https://github.com/aarontburn/nexus-tft-tactics-tools",
    "git-latest": {
        "git-username": "aarontburn",
        "git-repo-name": "nexus-tft-tactics-tools"
    },
    "build": {
        "build-version": 1,
        "excluded": [
            "electron.ts"
        ],
        "included": [],
        "process": "./process/main.js",
        "replace": [
            {
                "from": "{EXPORTED_MODULE_ID}",
                "to": "%id%",
                "at": [
                    "./process/main.ts"
                ]
            },
            {
                "from": "{EXPORTED_MODULE_NAME}",
                "to": "%name%",
                "at": [
                    "./process/main.ts"
                ]
            }
        ]
    }
}